<script type="text/javascript">
    var lunBoAnNiuClickJiShu = 0, lunBoJinMeiDanPinAnNiuShu = 0;
    $(function () {
        //搭配选项切换
        $("#ulDaPeiFenLei li").each(function (index, item) {
            $(item).hover(function () {
                switch ($(item).html()) {
                    case "耳机音箱":
                        DaPeiShow("erJi");
                        break;
                    case "数据线":
                        DaPeiShow("shuJuXian");
                        break;
                    case "电源":
                        break;
                    case "电池储存卡":
                        break;
                    default:
                };
            });
        });

        //竖排导航每个li中的内容出现和隐藏
        $("#divShuPaiFeiLeiDaoHang ul li div").each(function (index, item) {
            var $d = $(item);
            var bcolor = "#00ff21", width = "300px";
            if (index % 2 == 0) {
                bcolor = "#f19a9a";
                width = "400px";
            }
            //$d.css({ "left": $(window).width() * 0.12 + $d.parents("div").width(), "top": $d.parents("div").offset().top, "background-color": bcolor, "width": width }).html(index);
            $d.css({ "left": $(window).width() * 0.12 + $d.parents("div").width(), "top": $d.parents("div").offset().top });

            $d.parent().mousemove(function () {
                $(this).children("div").show();
            });
            $d.parent().mouseout(function () {
                $(this).children("div").hide();
            });
            $d.mousemove(function () {
                $(this).show();
            });
            $d.mouseout(function () {
                $(this).hide();
            });
        });

        ////大图轮播
        $("#divLunBoImgWai").ZSImgLunBo({
            jsonData: '{"data":[{"href":"","src":"http://i3.mifile.cn/a4/f4bee59d-85a5-498f-ae57-a2cabe6aeb5b"},{"href":"","src":"http://i3.mifile.cn/a4/f176643a-7db7-42e9-a4e5-f4d3bf9720f5 2x"},{"href":"","src":"http://i3.mifile.cn/a4/467eeee4-0e6b-41a0-b789-4373fa45a5a6 2x"},{"href":"","src":"http://i3.mifile.cn/a4/3d0a39fe-082f-420b-8857-eadd3b2e148c 2x"}]}',
            LunBoFangShi: "fade"
        });

        //精美单品击轮播
        $("#divJingMeiDanPinContent").mousemove(function () {
            clearInterval(jingMeiDanPInZiDongLunBoID);
        });
        $("#divJingMeiDanPinContent").mouseout(function () {
            JingMeiDanPinZiDongLunBo();
        });
        $("#divJingMeiDanPin ul li").each(function (index, item) {
            var $i = $(item);
            if (index == 0) {
                //后退
                $i.click(function () {
                    clearInterval(jingMeiDanPInZiDongLunBoID);
                    if (lunBoJinMeiDanPinAnNiuShu == 0) {
                        lunBoJinMeiDanPinAnNiuShu = $("#divJingMeiDanPinContent ul").length - 1;
                    } else {
                        lunBoJinMeiDanPinAnNiuShu--;
                    }
                    $("#divJingMeiDanPinContent ul").each(function () {
                        $(this).hide();
                    });
                    $("#divJingMeiDanPinContent ul").eq(lunBoJinMeiDanPinAnNiuShu).show();
                    JingMeiDanPinZiDongLunBo();
                });
            } else {
                //前进
                $i.click(function () {
                    clearInterval(jingMeiDanPInZiDongLunBoID);
                    if ($("#divJingMeiDanPinContent ul").length - 1 == lunBoJinMeiDanPinAnNiuShu) {
                        lunBoJinMeiDanPinAnNiuShu = 0;
                    } else {
                        lunBoJinMeiDanPinAnNiuShu++;
                    }
                    $("#divJingMeiDanPinContent ul").each(function () {
                        $(this).hide();
                    });
                    $("#divJingMeiDanPinContent ul").eq(lunBoJinMeiDanPinAnNiuShu).show();
                    JingMeiDanPinZiDongLunBo();
                });
            };
            $(this).mousemove(function () {
                $("#divJingMeiDanPinContent").mousemove();
            });
            $(this).mouseout(function () {
                $("#divJingMeiDanPinContent").mouseout();
            })
        });
        JingMeiDanPinZiDongLunBo();

        //搭配描述的出现与消失
        $(".ulDaPeiN li").mouseover(function () {
            $(this).find(".daPeiMiaoShu").show();
        });
        $(".ulDaPeiN li").mouseout(function () {
            $(this).find(".daPeiMiaoShu").hide();
        });




        //图片的大小超过容器后更改图片的大小为容器的大小
        //TuPianZiShiYing("img");
        TuPianZiShiYing(".divZSLunBoImg");
        TuPianZiShiYing("#ulKuaiSuZhiDa3GeImgLianJie");
    });

    function DaPeiShow(leiname) {
        $(".ulDaPeiN").each(function (index, item) {
            if ($(item).attr("leiname") == leiname) {
                $(item).show();
            } else {
                $(item).hide();
            }
        });
    };

    function TuPianZiShiYing(select) {
        if (select.toLocaleLowerCase != "img") {
            select += " img";
        }
        $(select).each(function (index, item) {
            $(item).height($(item).parents("div").height());
            $(item).width($(item).parents("div").width());
        });
    };

    //精美单品自动轮播和前进一样
    function JingMeiDanPinZiDongLunBo() {
        jingMeiDanPInZiDongLunBoID = setInterval(function () {
            if ($("#divJingMeiDanPinContent ul").length - 1 == lunBoJinMeiDanPinAnNiuShu) {
                lunBoJinMeiDanPinAnNiuShu = 0;
            } else {
                lunBoJinMeiDanPinAnNiuShu++;
            }
            $("#divJingMeiDanPinContent ul").each(function () {
                $(this).hide();
            });
            $("#divJingMeiDanPinContent ul").eq(lunBoJinMeiDanPinAnNiuShu).show();
        }, 3000);

    };

</script>









