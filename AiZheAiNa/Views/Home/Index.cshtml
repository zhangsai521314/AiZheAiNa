<script id="divShuPaiFeiLeiDaoHang_Template" type="text/x-handlebars">
    <!--竖排分类导航模板-->
    <ul class="list-unstyled text-center divShuPaiFeiLeiDaoHang_ul1">
        {{#each this}}
        <li class="divShuPaiFeiLeiDaoHang_li1">
            <a href="/AllGoods/{{AshuPaiDaoHangNameEN}}" class="divShuPaiFeiLeiDaoHang_a1">{{AshuPaiDaoHangName}}</a>
            <div class="divShuPaiFeiLeiDaoHang_div1">

                <ul class="list-unstyled divShuPaiFeiLeiDaoHang_ul2 divShuPaiFeiLeiDaoHang_ul2_{{@@index}}">
                    <!--进入属性-->
                    {{#with Specific}}
                    {{#each this}}
                    <li class="divShuPaiFeiLeiDaoHang_li2">
                        {{#if isShowDetails}}
                        <!--是否有大图介绍，类似http://www.mi.com/yinxiang/-->
                        <a href="/Details/{{shopType}}?shopModel={{shopModel}}" class="divShuPaiFeiLeiDaoHang_a2">
                            <img src="{{shuPaiDaoHangLiXiaoImgSrc}}" />
                            <span>{{shuPaiDaoHangLiXiaoImgSpanMiaoShu}}</span>
                        </a>
                        {{else}}
                        <a href="/Buy/{{shopType}}?shopModel={{shopModel}}" class="divShuPaiFeiLeiDaoHang_a2">
                            <img src="{{shuPaiDaoHangLiXiaoImgSrc}}" />
                            <span>{{shuPaiDaoHangLiXiaoImgSpanMiaoShu}}</span>
                        </a>
                        {{/if}}
                        {{#if buyName }}
                        <a href="/Buy/{{shopType}}?shopModel={{shopModel}}" class="divShuPaiFeiLeiDaoHang_a3">{{buyName}}</a>
                        {{/if}}
                    </li>
                    {{/each}}
                    {{/with}}
                </ul>
            </div>
        </li>
        {{/each}}
    </ul>
</script>
