</script>
<script id="divDaPeiRight_Template" type="text/x-handlebars">
    <!--搭配右边内容模板,@@last表示最后的循环。其实handlebars应该的是@只不过这是razor视图格式所以需要转换-->
    <ul class="text-center list-inline ulDaPeiN" FenLeiEnName="{{FenLeiEnName}}">
        {{#with Content}}
        {{#each this}}
        {{#if @@last}}
        <li style="width:240px;height:140px;position:relative;top:-122px">
            <div style="width:230px;height:150px;">
                <div style="height: 60px;width: 130px;position:absolute;top:55px">
                    <a href="{{aDaPeiNameLink}}">{{Name}}</a><br />
                    <span>{{PingJiaRenName}}</span>
                </div>
                <div style="height: 80px;width: 80px;position:absolute;right:20px;top:30px">
                    <a herf="{{aDaPeiImgLink}}">
                        <img style="width:80px;height:80px" alt="{{ImgAlt}}" src="{{ImgSrc}}" />
                    </a>
                </div>
            </div>
        </li>
        {{else}}
        <li>
            <div class="divDaPeiRightContent">
                {{#if TeDian}}
                <!--没有特点则不生成特点div-->
                <div class="divDaPeiRightMiaoShu3">
                    {{TeDian}}
                </div>
                {{/if}}
                <a herf="{{aDaPeiImgLink}}">
                    <img alt="{{ImgAlt}}" src="{{ImgSrc}}" />
                </a>
                <div class="divDaPeiRightMiaoShu1">
                    <a href="{{aDaPeiNameLink}}">{{Name}}</a><p />
                    <p>{{JiaGe}}</p>
                    <p>{{PingJiaShuLiang}}</p>
                </div>
                {{#if PingJia}}
                <!--没有评价则不生成评价div-->
                <div class="divDaPeiRightMiaoShu2">
                    <a href="{{aDaPeiMiaoShu2Link}}">
                        <p>{{PingJia}}</p>
                        <p>{{PingJiaRenName}}</p>
                    </a>
                </div>
                {{/if}}
            </div>
        </li>
        {{/if}}
        {{/each}}
        {{/with}}
        <li style="width:240px;height:150px;position:absolute;right:-9px;bottom:-19px">
            <div style="width:230px;height:150px;position:absolute;right:22px;bottom:20px">
                <div style="height: 80px;width: 130px;position:absolute;top:50px">
                    <a href="">浏览更多</a><p />
                    <p>{{FenLeiCnName}}</p>
                </div>
                <div style="height: 80px;width: 80px;position:absolute;right:20px;top:35px">
                    <a herf="{{aDaPeiImgLink}}">
                        <img style="width:90px;height:80px" src="./Content/Images/go.png" />
                    </a>
                </div>
            </div>
        </li>
    </ul>
</script>
