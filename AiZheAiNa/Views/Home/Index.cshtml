<script src="~/Scripts/jq/jquery-2.1.4/jquery.js"></script>
<script type="text/javascript">
    $(function () {

        $("img").each(function (index, item) {
            $(item).height($(item).parent().height());
            $(item).width($(item).parent().width());
        });

    })
</script>
<div style="width: 100%">
