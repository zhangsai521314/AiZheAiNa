    function DaPeiShow(FenLeiEnName) {
        var isYiJiaZai = false;
        $(".ulDaPeiN").each(function (index, item) {
            if ($(item).attr("FenLeiEnName") == FenLeiEnName) {
                isYiJiaZai = true;
                return false;
            }
        });
        if (isYiJiaZai) {
            $(".ulDaPeiN").each(function (index, item) {
                if ($(item).attr("FenLeiEnName") == FenLeiEnName) {
                    $(item).show();
                } else {
                    $(item).hide();
                }
            });
        } else {
            //此处相当于ajax请求后台获取数据
            if (FenLeiEnName == "erJi") {
                //相当于请求成功后，否则后死循环
                $("#divDaPeiRight").html($("#divDaPeiRight").html() + divDaPeiRight_Template(divDaPeiRight2));
                $(".my-ultempplate-div1").hover(function () {
                    $(this).find(".divDaPeiRightMiaoShu2").css({ "height": "90px", "opacity": 1 });
                }, (function () {
                    $(this).find(".divDaPeiRightMiaoShu2").css({ "height": "0px", "opacity": 1 });
                }));
                DaPeiShow(FenLeiEnName);
            }
        };
    };

    function TuPianZiShiYing(select) {
        if (select.toLocaleLowerCase != "img") {
            select += " img";
        }
        $(select).each(function (index, item) {
            $(item).height($(item).parents("div").height());
            $(item).width($(item).parents("div").width());
        });
    };
</script>









