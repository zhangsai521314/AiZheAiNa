<script id="divShuPaiFeiLeiDaoHang_Template" type="text/x-handlebars">
    @*竖排分类导航模板*@
    <ul class="list-unstyled text-center divShuPaiFeiLeiDaoHang_ul1">
        {{#each this}}
        <li class="divShuPaiFeiLeiDaoHang_li1">
            <a href="{{AshuPaiDaoHangLink}}" class="divShuPaiFeiLeiDaoHang_a1">{{AshuPaiDaoHangName}}</a>
            <div class="divShuPaiFeiLeiDaoHang_div1">
                <ul class="list-inline divShuPaiFeiLeiDaoHang_ul2">
                    @*进入属性*@
                    {{#with Specific}}
                    {{#each this}}
                    <li class="divShuPaiFeiLeiDaoHang_li2">
                        <a href="{{XiangQing}}" class="divShuPaiFeiLeiDaoHang_a2">
                            <img src="{{shuPaiDaoHangLiXiaoImgSrc}}" />
                            <span>{{shuPaiDaoHangLiXiaoImgSpanMiaoShu}}</span>
                        </a>
                        {{#if zhiDaName }}
                        <a href="{{zhiDaLink}}" class="divShuPaiFeiLeiDaoHang_a3">{{zhiDaName}}</a>
                        {{/if}}
                    </li>
                    {{/each}}
                    {{/with}}
                </ul>
            </div>
        </li>
        {{/each}}
    </ul>
</script>
<script id="divJingMeiDanPinContent_Template" type="text/x-handlebars">
