<script type="text/javascript">
    $(function () {

        //图片的大小超过容器后更改图片的大小为容器的大小
        $("img").each(function (index, item) {
            if ($(item).height() > $(item).parent().height()) {
                $(item).height($(item).parent().height());
            }
            if ($(item).width() > $(item).parent().width()) {
                $(item).width($(item).parent().width());
            }
        });
        $(".ulDaPeiFenLei li").each(function (index, item) {
            $(item).hover(function () {
                switch ($(item).html()) {
                    case "耳机音箱":
                        DaPeiShow("erJi");
                        break;
                    case "数据线":
                        DaPeiShow("shuJuXian");
                        break;
                    case "电源":
                        break;
                    case "电池储存卡":
                        break;
                    default:
                };
            });
        });

        $(".ulDaPeiFenLei").mouseout(function () {
            DaPeiShow("erJi");
        });
    });
    function DaPeiShow(leiname) {
        $(".DaPei").each(function (index, item) {
            if ($(item).attr("leiname") == leiname) {
                $(item).show();
            } else {
                $(item).hide();
            }
        });
    };
</script>
